{
  "framework": "create-react-app",
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "outputDirectory": "build",
  "public": true,
  "env": {
    "REACT_APP_API_URL": "/api",
    "REACT_APP_USE_MOCK_API": "false",
    "BACKEND_API_URL": "https://registry.reviz.dev/api"
  },
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    },
    {
      "src": "/api/test-backend",
      "dest": "/api/test-backend",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "src": "/api/test-real-backend",
      "dest": "/api/test-real-backend",
      "methods": ["GET", "OPTIONS"]
    },
    {
      "src": "/api/debug",
      "dest": "/api/test-backend",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "src": "/api/backend-url",
      "dest": "/api/backend-url",
      "methods": ["GET", "OPTIONS"]
    },
    {
      "src": "/api/config",
      "dest": "/api/backend-url",
      "methods": ["GET", "OPTIONS"]
    },
    {
      "src": "/api/proxy-debug",
      "dest": "/api/proxy-debug",
      "methods": ["GET", "OPTIONS"]
    },
    {
      "src": "/api/auth-debug",
      "dest": "/api/auth-debug",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "src": "/api/test-proxy-html",
      "dest": "/api/test-proxy-html",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "src": "/api/serve-local",
      "dest": "/api/serve-local",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "src": "/api/auth",
      "dest": "/api/auth-proxy",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    },
    {
      "src": "/api/auth/login",
      "dest": "/api/auth-proxy",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    },
    {
      "src": "/api/auth/register",
      "dest": "/api/auth-proxy", 
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    },
    {
      "src": "/api/auth/profile",
      "dest": "/api/auth-proxy",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    },
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/auth-proxy",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    },
    {
      "src": "/api/assets",
      "dest": "/api/assets",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    },
    {
      "src": "/api/assets/(.*)",
      "dest": "/api/assets",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/proxy?path=$1",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}